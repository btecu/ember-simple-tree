{
  "version": "0.9.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btecu/ember-simple-tree.git"
  },
  "license": "MIT",
  "author": "Bj Tecu",
  "workspaces": [
    "ember-simple-tree",
    "test-app"
  ],
  "scripts": {
    "build": "pnpm --filter ember-simple-tree build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefixColors auto",
    "start:addon": "pnpm --filter ember-simple-tree start --no-watch.clearScreen",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.6.2",
    "prettier-plugin-ember-template-tag": "^2.1.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "packageManager": "pnpm@10.20.0",
  "volta": {
    "node": "22.21.1",
    "pnpm": "10.20.0"
  }
}
