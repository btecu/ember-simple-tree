{
  "version": "0.9.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/btecu/ember-simple-tree.git"
  },
  "license": "MIT",
  "author": "Bj Tecu",
  "workspaces": [
    "ember-simple-tree",
    "test-app"
  ],
  "scripts": {
    "build": "yarn workspace ember-simple-tree run build",
    "lint": "yarn workspaces foreach -A run lint",
    "lint:fix": "yarn workspaces foreach -A run lint:fix",
    "prepare": "yarn build",
    "start": "concurrently \"yarn:start:*\" --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "yarn workspace ember-simple-tree run start",
    "start:test-app": "yarn workspace test-app run start",
    "test": "yarn workspaces foreach -A run test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "volta": {
    "node": "22.14.0",
    "yarn": "4.7.0"
  },
  "packageManager": "yarn@4.7.0"
}
