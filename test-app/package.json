{
  "name": "test-app",
  "version": "0.9.1",
  "description": "Lightweight, composable tree component for Ember",
  "keywords": [
    "ember-cli",
    "ember-tree",
    "tree"
  ],
  "repository": "https://github.com/btecu/ember-simple-tree",
  "license": "MIT",
  "author": "Bj Tecu",
  "contributors": [
    "Aaron Heidorn",
    "Corey Wells",
    "Gaurav Munjal",
    "Teddy Coleman"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\" --allow-empty-input --cache\"",
    "lint:css:fix": "stylelint \"**/*.css\" --allow-empty-input --fix",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:ember-compatibility": "ember try:one"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@ember/optional-features": "^2.2.0",
    "@ember/test-helpers": "^5.2.0",
    "@embroider/broccoli-side-watch": "^1.0.1",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.23.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@stylistic/eslint-plugin": "^4.2.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.1.2",
    "ember-auto-import": "^2.10.0",
    "ember-cli": "^6.2.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^8.1.0",
    "ember-resolver": "^12.0.1",
    "ember-simple-tree": "0.9.0",
    "ember-source": "^6.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.0.1",
    "ember-try": "^4.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-ember": "^12.5.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "prettier": "^3.5.3",
    "qunit": "^2.24.1",
    "qunit-dom": "^3.4.0",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-prettier": "^5.0.3",
    "webpack": "^5.98.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
